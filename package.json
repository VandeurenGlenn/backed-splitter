{
  "name": "backed-splitter",
  "version": "1.5.0",
  "repository": "https://github.com/VandeurenGlenn/backed-crisper.git",
  "author": "Glenn Vandeuren <vandeurenglenn@gmail.com>",
  "scripts": {
    "test": "mocha"
  },
  "files": [
    "lib"
  ],
  "main": "lib/splitter-node",
  "jsnext": "lib/splitter-es",
  "backed": {
    "bundles": [
      {
        "src": "src/index.js",
        "dest": "lib/splitter.js",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ],
            "plugins": [
              "external-helpers",
              "transform-async-generator-functions"
            ]
          }
        }
      },
      {
        "src": "src/index.js",
        "dest": "lib/splitter.js",
        "format": "cjs",
        "plugins": {
          "babel": {
            "presets": [
              [
                "es2015",
                {
                  "modules": false
                }
              ]
            ],
            "plugins": [
              "transform-async-generator-functions",
              "external-helpers",
              [
                "transform-runtime",
                {
                  "polyfill": false,
                  "regenerator": true
                }
              ]
            ]
          }
        }
      }
    ],
    "sources": [
      {
        "src": [
          "node_modules/babel-runtime/helpers/{classCallCheck,createClass,regenerator,asyncGenerator}*.js",
          "node_modules/babel-runtime/regenerator/*.js",
          "node_modules/babel-runtime/core-js/object/define-property.js"
        ],
        "dest": "lib/node_modules"
      }
    ]
  },
  "dependencies": {
    "backed-utils": "^0.0.2",
    "dom5": "^2.2.0",
    "micromatch": "^3.0.0",
    "parse5": "^3.0.2"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "mocha": "^3.2.0"
  },
  "license": "MIT"
}
